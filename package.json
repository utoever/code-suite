{
  "name": "codesuite",
  "displayName": "Code Suite",
  "description": "Code support tool",
  "version": "0.0.1",
  "author": "utocode",
  "publisher": "utocode",
  "icon": "images/logo.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/utocode/code-suite"
  },
  "license": "GNU General Public License v2.0",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Formatters",
    "Language Packs",
    "Programming Languages",
    "Extension Packs",
    "Other"
  ],
  "capabilities": {
    "virtualWorkspaces": true
  },
  "activationEvents": [],
  "files": [
    "dist/",
    "media/",
    "images/",
    "snippets/",
    "resources/",
    "extension.js",
    "extension.css",
    "*.md"
  ],
  "main": "./dist/extension.js",
  "keywords": [
    "code",
    "management"
  ],
  "contributes": {
    "commands": [
      {
        "command": "codesuite.transformTo",
        "title": "Transform to",
        "category": "codesuite"
      },
      {
        "command": "codesuite.transformToCamelcase",
        "title": "Transform To Camelcase",
        "category": "codesuite"
      },
      {
        "command": "codesuite.transformToSnakecase",
        "title": "Transform To Snakecase",
        "category": "codesuite"
      },
      {
        "command": "codesuite.transformToJavaCase",
        "title": "Transform To JavaCase",
        "category": "codesuite"
      },
      {
        "command": "codesuite.transformToJavaCaseSmart",
        "title": "Transform To JavaCase",
        "category": "codesuite"
      },
      {
        "command": "codesuite.helloWorld",
        "title": "Hello World",
        "category": "codesuite"
      }
    ],
    "keybindings": [
      {
        "command": "codesuite.transformTo",
        "key": "alt+f1",
        "mac": "option+f1",
        "when": "editorTextFocus"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "webpack": "webpack",
    "compile": "webpack",
    "watch": "webpack --watch",
    "compile0": "tsc -p ./",
    "watch0": "tsc -watch -p ./",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./dist/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/lodash": "^4.14.195",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@types/vscode": "^1.74.0",
    "@typescript-eslint/eslint-plugin": "^5.59.1",
    "@typescript-eslint/parser": "^5.59.1",
    "@vscode/test-electron": "^2.3.0",
    "eslint": "^8.39.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "ts-loader": "^9.4.3",
    "typescript": "^5.0.4",
    "webpack": "^5.88.0",
    "webpack-cli": "^5.0.2",
    "webpack-dev-server": "^4.15.1"
  },
  "dependencies": {
    "html-entities": "^2.3.6",
    "lodash": "^4.17.21"
  }
}
